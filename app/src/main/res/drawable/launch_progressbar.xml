<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#8b000000" android:endColor="#8b000000" android:angle="270" android:centerY="1" android:centerColor="#8b000000"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#d483f6" android:endColor="#8b000000" android:angle="270" android:centerY="1" android:centerColor="#d483f6"/>
            </shape>
        </clip>
    </item>
</layer-list>
